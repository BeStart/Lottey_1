<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:background="#66CDAA"
            android:orientation="horizontal" >

            <LinearLayout
                android:id="@+id/base_back"
                android:layout_width="fill_parent"
                android:layout_height="match_parent"
                android:layout_weight="2"
                android:background="#66CDAA"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:background="@drawable/back_btn"
                    android:gravity="center" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#66CDAA"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/base_tickettype"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="重庆时时彩"
                    android:textColor="#ffffff"
                    android:textSize="20dp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/LinearLayout_ticket_rule"
                android:layout_width="fill_parent"
                android:layout_height="match_parent"
                android:layout_weight="2"
                android:background="#66CDAA"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:background="@drawable/icon_info" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:background="#66CDAA"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="39.8dp"
                android:layout_weight="1"
                android:background="#ffffff"
                android:orientation="vertical" >

                <HorizontalScrollView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scrollbars="none" >

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:orientation="horizontal" >

                        <Button
                            android:id="@+id/btn_menu1"
                            android:layout_width="wrap_content"
                            android:layout_height="25dp"
                            android:layout_marginLeft="15dp"
                            android:layout_marginRight="15dp"
                            android:background="#ffffff"
                            android:paddingLeft="3dp"
                            android:paddingRight="3dp"
                            android:text="四星直选一位微微"
                            android:textColor="#303030"
                            android:textSize="15dp" />

                        <Button
                            android:id="@+id/btn_menu2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="15dp"
                            android:layout_marginRight="15dp"
                            android:background="#ffffff"
                            android:paddingLeft="3dp"
                            android:paddingRight="3dp"
                            android:text="btn2sdfsf"
                            android:textColor="#303030"
                            android:textSize="15dp" />

                        <Button
                            android:id="@+id/btn_menu3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="15dp"
                            android:layout_marginRight="15dp"
                            android:background="#ffffff"
                            android:paddingLeft="3dp"
                            android:paddingRight="3dp"
                            android:text="btn3sfsf"
                            android:textColor="#303030"
                            android:textSize="15dp" />

                        <Button
                            android:id="@+id/btn_menu4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="15dp"
                            android:layout_marginRight="15dp"
                            android:background="#ffffff"
                            android:paddingLeft="3dp"
                            android:paddingRight="3dp"
                            android:text="btn4dfsf"
                            android:textColor="#303030"
                            android:textSize="15dp" />

                        <Button
                            android:id="@+id/btn_menu5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="15dp"
                            android:layout_marginRight="15dp"
                            android:background="#ffffff"
                            android:paddingLeft="3dp"
                            android:paddingRight="3dp"
                            android:text="btnsfsdfs5"
                            android:textColor="#303030"
                            android:textSize="15dp" />
                    </LinearLayout>
                </HorizontalScrollView>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/play_menu_arrow"
                android:layout_width="fill_parent"
                android:layout_height="39.8dp"
                android:layout_weight="7"
                android:background="#FAFAFA"
                android:gravity="center" >

                <ImageButton
                    android:id="@+id/play_menu_arrow1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@android:color/transparent" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:background="#FAFAFA"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="30dp"
                android:layout_weight="0.9"
                android:background="#FAFAFA"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:singleLine="true"
                    android:text="当前期："
                    android:textColor="#66CDAA"
                    android:textSize="12dp" />

                <TextView
                    android:id="@+id/text_showno"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:singleLine="true"
                    android:text="20150924-29"
                    android:textColor="#66CDAA"
                    android:textSize="12dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:background="#FAFAFA"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/littleclock" />

                <TextView
                    android:id="@+id/text_showtime"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:singleLine="true"
                    android:text="201509"
                    android:textColor="#66CDAA"
                    android:textSize="12dp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/LinearLayout_rocknumber"
                android:layout_width="fill_parent"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:background="#FAFAFA"
                android:gravity="center" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="17dp"
                    android:background="@drawable/shake_icon" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:singleLine="true"
                    android:text="摇一摇"
                    android:textColor="#66CDAA"
                    android:textSize="12dp" />
            </LinearLayout>
        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_weight="1" >

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent" >

                        <LinearLayout
                            android:id="@+id/content"
                            android:layout_width="match_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:orientation="vertical" >
                        </LinearLayout>
                    </RelativeLayout>
                </LinearLayout>

                <RelativeLayout
                    android:id="@+id/RelativeLayout_addbasket"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:background="@drawable/pick_num_bottombg" >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal" >

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:background="@android:color/transparent" >

                            <LinearLayout
                                android:id="@+id/LinearLayout_ischoose"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_gravity="center_horizontal"
                                android:visibility="gone" >

                                <ImageButton
                                    android:id="@+id/imagebtn_addbasket"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_vertical"
                                    android:background="@drawable/basket_add"
                                    android:visibility="visible" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:orientation="vertical"
                                    android:visibility="visible" >

                                    <TextView
                                        android:id="@+id/text_chooseend"
                                        android:layout_width="match_parent"
                                        android:layout_height="fill_parent"
                                        android:layout_gravity="center_vertical"
                                        android:layout_marginLeft="10dp"
                                        android:layout_weight="1"
                                        android:background="@android:color/transparent"
                                        android:gravity="bottom"
                                        android:text="已选100000嘱，公200000.2元"
                                        android:textColor="#ffffff"
                                        android:textSize="15dp"
                                        android:visibility="visible" />

                                    <TextView
                                        android:id="@+id/text_chooseresult"
                                        android:layout_width="match_parent"
                                        android:layout_height="fill_parent"
                                        android:layout_gravity="center_vertical"
                                        android:layout_marginLeft="10dp"
                                        android:layout_weight="1"
                                        android:background="@android:color/transparent"
                                        android:singleLine="true"
                                        android:text="1234512345122345134214"
                                        android:textColor="#ffffff"
                                        android:textSize="12dp"
                                        android:visibility="visible" />
                                </LinearLayout>
                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/LinearLayout_nochoose"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_gravity="center_horizontal"
                                android:layout_marginLeft="10dp"
                                android:visibility="visible" >

                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:orientation="vertical" >

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="fill_parent"
                                        android:layout_gravity="center_vertical"
                                        android:layout_marginLeft="10dp"
                                        android:layout_weight="1"
                                        android:background="@android:color/transparent"
                                        android:gravity="bottom"
                                        android:text="已选0注，共0.0元"
                                        android:textColor="#ffffff"
                                        android:textSize="15dp" />

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="fill_parent"
                                        android:layout_gravity="center_vertical"
                                        android:layout_marginLeft="10dp"
                                        android:layout_weight="1"
                                        android:background="@android:color/transparent"
                                        android:text="每位选一个号码为一注"
                                        android:textColor="#ffffff"
                                        android:textSize="12dp" />
                                </LinearLayout>
                            </LinearLayout>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent" >

                            <RelativeLayout
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_alignParentRight="true"
                                android:background="@drawable/btn_grey" >

                                <LinearLayout
                                    android:id="@+id/LinearLayout_numberbasket"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:layout_marginLeft="30dp" >

                                    <ImageButton
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center_vertical"
                                        android:background="@drawable/basket_icon" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center_vertical"
                                        android:layout_marginLeft="10dp"
                                        android:background="@android:color/transparent"
                                        android:text="号码篮"
                                        android:textColor="#ffffff"
                                        android:textSize="18dp" />

                                    <TextView
                                        android:id="@+id/TextView_show_number"
                                        android:layout_width="20dp"
                                        android:layout_height="20dp"
                                        android:layout_marginRight="10dp"
                                        android:layout_marginTop="15dp"
                                        android:background="@drawable/badge"
                                        android:gravity="center"
                                        android:text="99+"
                                        android:textColor="#ffffff"
                                        android:textSize="10dp"
                                        android:visibility="invisible" />
                                </LinearLayout>
                            </RelativeLayout>
                        </LinearLayout>
                    </LinearLayout>
                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/tran_sur"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#b0000000"
                android:orientation="vertical"
                android:visibility="gone" >
            </LinearLayout>

            <LinearLayout
                android:id="@+id/play_menu"
                android:layout_width="match_parent"
                android:layout_height="400dp"
                android:background="#ffffff"
                android:orientation="vertical"
                android:visibility="visible" >

                <ScrollView
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:scrollbars="vertical" >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="400dp"
                        android:background="#C5C1AA"
                        android:orientation="vertical" >

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:background="#ffffff"
                            android:orientation="horizontal" >

                            <TextView
                                android:layout_width="80dp"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:text="三星直选"
                                android:textColor="#000000" />

                            <TextView
                                android:id="@+id/three_direct_double_text"
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="复式"
                                android:textColor="#8A8A8A" />

                            <TextView
                                android:id="@+id/three_direct_single_text"
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="单式"
                                android:textColor="#8A8A8A" />

                            <TextView
                                android:id="@+id/three_direct_sum_text"
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="和值"
                                android:textColor="#8A8A8A" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:layout_marginTop="0.4dp"
                            android:background="#ffffff"
                            android:orientation="horizontal" >

                            <TextView
                                android:layout_width="80dp"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:text="三星组选"
                                android:textColor="#000000" />

                            <TextView
                                android:id="@+id/three_group_three_text"
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="组三"
                                android:textColor="#8A8A8A" />

                            <TextView
                                android:id="@+id/three_group_six_text"
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="组六"
                                android:textColor="#8A8A8A" />

                            <TextView
                                android:id="@+id/three_group_sum_text"
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="和值"
                                android:textColor="#8A8A8A" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:background="#ffffff"
                            android:orientation="horizontal" >

                            <TextView
                                android:layout_width="80dp"
                                android:layout_height="match_parent" />

                            <TextView
                                android:id="@+id/three_group_combination_text"
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="混合组选"
                                android:textColor="#8A8A8A" />

                            <TextView
                                android:id="@+id/three_group_three_single_text"
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="组三单式"
                                android:textColor="#8A8A8A" />

                            <TextView
                                android:id="@+id/three_group_six_single_text"
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="组六单式"
                                android:textColor="#8A8A8A" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:layout_marginTop="0.4dp"
                            android:background="#ffffff"
                            android:orientation="horizontal" >

                            <TextView
                                android:layout_width="80dp"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:text="前二直选"
                                android:textColor="#000000" />

                            <TextView
                                android:id="@+id/top_two_direct_double_text"
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="复式"
                                android:textColor="#8A8A8A" />

                            <TextView
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:textColor="#8A8A8A" />

                            <TextView
                                android:id="@+id/top_two_direct_single_text"
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="单式"
                                android:textColor="#8A8A8A" />

                            <TextView
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:textColor="#8A8A8A" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:layout_marginTop="0.4dp"
                            android:background="#ffffff"
                            android:orientation="horizontal" >

                            <TextView
                                android:layout_width="80dp"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:text="前二组选"
                                android:textColor="#000000" />

                            <TextView
                                android:id="@+id/top_two_group_double_text"
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="复式"
                                android:textColor="#8A8A8A" />

                            <TextView
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:textColor="#8A8A8A" />

                            <TextView
                                android:id="@+id/top_two_group_single_text"
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="单式"
                                android:textColor="#8A8A8A" />

                            <TextView
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:textColor="#8A8A8A" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:layout_marginTop="0.4dp"
                            android:background="#ffffff"
                            android:orientation="horizontal" >

                            <TextView
                                android:layout_width="80dp"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:text="后二直选"
                                android:textColor="#000000" />

                            <TextView
                                android:id="@+id/bottom_two_direct_double_text"
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="复式"
                                android:textColor="#8A8A8A" />

                            <TextView
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:textColor="#8A8A8A" />

                            <TextView
                                android:id="@+id/bottom_two_direct_single_text"
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="单式"
                                android:textColor="#8A8A8A" />

                            <TextView
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:textColor="#8A8A8A" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:layout_marginTop="0.4dp"
                            android:background="#ffffff"
                            android:orientation="horizontal" >

                            <TextView
                                android:layout_width="80dp"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:text="后二组选"
                                android:textColor="#000000" />

                            <TextView
                                android:id="@+id/bottom_two_group_double_text"
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="复式"
                                android:textColor="#8A8A8A" />

                            <TextView
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:textColor="#8A8A8A" />

                            <TextView
                                android:id="@+id/bottom_two_group_single_text"
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="单式"
                                android:textColor="#8A8A8A" />

                            <TextView
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:textColor="#8A8A8A" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:layout_marginTop="0.4dp"
                            android:background="#ffffff"
                            android:orientation="horizontal" >

                            <TextView
                                android:layout_width="90dp"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:text="定位胆"
                                android:textColor="#000000" />

                            <TextView
                                android:id="@+id/fixedposition_text"
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="定位胆"
                                android:textColor="#8A8A8A" />

                            <TextView
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:textColor="#8A8A8A" />

                            <TextView
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:textColor="#8A8A8A" />

                            <TextView
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:textColor="#8A8A8A" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:layout_marginTop="0.4dp"
                            android:background="#ffffff"
                            android:orientation="horizontal" >

                            <TextView
                                android:layout_width="80dp"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:text="不定位"
                                android:textColor="#000000" />

                            <TextView
                                android:id="@+id/onenumber_notposition_text"
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="一码不定位"
                                android:textColor="#8A8A8A" />

                            <TextView
                                android:id="@+id/twonumber_notposition_text"
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="二码不定位"
                                android:textColor="#8A8A8A" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:layout_marginTop="0.4dp"
                            android:background="#ffffff"
                            android:orientation="horizontal" >

                            <TextView
                                android:layout_width="80dp"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:text="大小单双"
                                android:textColor="#000000" />

                            <TextView
                                android:id="@+id/maxmin_bottom_text"
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="后二大小单双"
                                android:textColor="#8A8A8A" />

                            <TextView
                                android:id="@+id/maxmin_toptwo_text"
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="前二大小单双"
                                android:textColor="#8A8A8A" />
                        </LinearLayout>
                    </LinearLayout>
                </ScrollView>
            </LinearLayout>
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/lin_loading"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="#b0000000"
        android:gravity="center"
        android:orientation="vertical"
        android:visibility="gone" >

        <LinearLayout
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:background="@drawable/loading_bg"
            android:gravity="center"
            android:orientation="vertical" >

            <Button
                android:id="@+id/loadingBtn"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:background="@drawable/loading" >
            </Button>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="loading..."
                android:textColor="#ffffff"
                android:textSize="18dp" />
        </LinearLayout>
    </LinearLayout>

</RelativeLayout>