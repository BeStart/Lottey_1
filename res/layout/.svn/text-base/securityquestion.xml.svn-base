<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    <LinearLayout 
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        >
        
        <include layout="@layout/title"/>
        
        
    </LinearLayout>
  <LinearLayout 
      android:background="#ffffff"
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="7"
        >
      <LinearLayout 
          android:orientation="vertical"
          android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
          >
         <LinearLayout 
             android:layout_width="fill_parent"
             android:layout_height="0dp"
             android:layout_weight="1"
             >
             <TextView 
                 android:layout_width="0dp"
                 android:layout_height="fill_parent"
                 android:layout_weight="1"
                 android:text="问题1"
                 android:layout_gravity="center"
                 android:gravity="center"
                 />
             <Spinner 
                 android:id="@+id/securityquestionask1"
                 android:layout_width="0dp"
                 android:layout_height="fill_parent"
                 android:layout_weight="4"
                 />
         </LinearLayout>
          <LinearLayout 
             android:layout_width="fill_parent"
             android:layout_height="0dp"
             android:layout_weight="1"
             >
             <TextView 
                 android:layout_width="0dp"
                 android:layout_height="fill_parent"
                 android:layout_weight="1"
                 android:text="答案"
                 android:layout_gravity="center"
                 android:gravity="center"
                 />
             <EditText 
                  android:id="@+id/securityquestionanswser1"
                 android:layout_width="0dp"
                 android:layout_height="fill_parent"
                 android:layout_weight="4"
                 />
         </LinearLayout>
          
          
      </LinearLayout>  
      <LinearLayout 
          android:orientation="vertical"
          android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
          
          
          >
          <LinearLayout 
             android:layout_width="fill_parent"
             android:layout_height="0dp"
             android:layout_weight="1"
             >
             <TextView 
                 android:layout_width="0dp"
                 android:layout_height="fill_parent"
                 android:layout_weight="1"
                 android:text="问题2"
                 android:layout_gravity="center"
                 android:gravity="center"
                 />
             <Spinner 
                 android:id="@+id/securityquestionask2"
                 android:layout_width="0dp"
                 android:layout_height="fill_parent"
                 android:layout_weight="4"
                 />
         </LinearLayout>
          <LinearLayout 
             android:layout_width="fill_parent"
             android:layout_height="0dp"
             android:layout_weight="1"
             >
             <TextView 
                 android:layout_width="0dp"
                 android:layout_height="fill_parent"
                 android:layout_weight="1"
                 android:text="答案"
                 android:layout_gravity="center"
                 android:gravity="center"
                 />
             <EditText 
                  android:id="@+id/securityquestionanswser2"
                 android:layout_width="0dp"
                 android:layout_height="fill_parent"
                 android:layout_weight="4"
                 />
         </LinearLayout>
          
          
          
      </LinearLayout>  
      <LinearLayout 
          android:orientation="vertical"
          android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="2"
          >
          <TextView 
              android:text="@string/securityquestiondetails"
              android:layout_width="fill_parent"
              android:layout_height="fill_parent"
              />
          
          
          
      </LinearLayout>  
      
        
        
        
    </LinearLayout>
</LinearLayout>
